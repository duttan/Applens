<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:background="#e09ab8"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <View
            android:layout_width="396dp"
            android:layout_height="250dp"
            android:background="#B32A2B2B"
            android:id="@+id/view3"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="2dp" android:layout_marginStart="2dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp" android:layout_marginRight="4dp" app:layout_constraintHorizontal_bias="0.555"
    />
    <TextView
            android:text="Applens"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView25"
            android:textColor="#3cbd3e"
            android:textStyle="bold" android:textSize="30sp" android:fontFamily="serif" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/view3" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="@+id/view3" android:layout_marginLeft="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/button"
            android:layout_marginRight="8dp"/>
    <Button
            android:text="Submit"
            style="@style/RoundedButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/view3" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/view3" android:layout_marginRight="8dp"/>
    <ImageView
            android:layout_width="359dp"
            android:layout_height="180dp"
            android:src="@drawable/final_tik"
            android:id="@+id/imageView5" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView25" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/view3" app:layout_constraintStart_toStartOf="@+id/view3"
            app:layout_constraintEnd_toEndOf="@+id/view3"/>
</androidx.constraintlayout.widget.ConstraintLayout>